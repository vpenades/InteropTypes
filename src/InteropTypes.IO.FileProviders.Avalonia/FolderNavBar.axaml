<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="InteropTypes.IO.FolderNavBar" x:Name="mySelf">
  
  <DockPanel>
    <Button Content="🏠" Click="OnClick_GoToAppDir" />
    <Button Content="↑" Click="OnClick_FolderUp" />
    <Button DockPanel.Dock="Right" Content="📁" Click="OnClick_FolderBrowse" />
    
    <TextBox x:Name="myCurrentPath" IsReadOnly="true" />
    
  </DockPanel>
    
</UserControl>
